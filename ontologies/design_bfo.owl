    <!-- http://www.ohio.edu/ontologies/design#ArtifactRepresentation -->

    <owl:Class rdf:about="http://www.ohio.edu/ontologies/design#ArtifactRepresentation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ohio.edu/ontologies/design#Representation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ohio.edu/ontologies/design#is_represented_by"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/Artifact"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ohio.edu/ontologies/design#Representation"/>
        <obo:IAO_0000112 xml:lang="en">a painting visioned by a painter</obo:IAO_0000112>
        <obo:IAO_0000112 xml:lang="en">the design of a clothing/accessory conceptualized by a fashion designer</obo:IAO_0000112>
        <obo:IAO_0000115 xml:lang="en">a Representation which is_about or is intended to be about an Artifact</obo:IAO_0000115>
        <obo:IAO_0000602 xml:lang="en">forall x (ArtifactRepresentation(x) -&gt; exists y (Aritfact(y) and is_about(x,y)))</obo:IAO_0000602>
        <rdfs:label xml:lang="en">Artifact Representation</rdfs:label>
    </owl:Class>
    


