    <!-- http://www.ohio.edu/ontologies/design#BoundaryRepresentation -->

    <owl:Class rdf:about="http://www.ohio.edu/ontologies/design#BoundaryRepresentation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ohio.edu/ontologies/design#Representation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ohio.edu/ontologies/design#is_represented_by"/>
